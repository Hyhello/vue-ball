{
    "name": "@hyhello/vue-ball",
    "version": "0.0.2",
    "description": "A shopping cart animation based on vue2.0",
    "keywords": [
        "ball",
        "@hyhello",
        "vue-ball",
        "Vuejs"
    ],
    "files": [
        "lib",
        "src",
        "packages"
    ],
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.umd.js",
    "scripts": {
        "bootstrap": "yarn || npm i",
        "deploy": "bash build/deploy.sh",
        "dev": "npm run serve",
        "prebuild": "npm run lint:fix",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
        "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue src",
        "ghpage": "yarn docs:build && node ./build/gh-pages.js",
        "serve": "cross-env NODE_ENV=development rollup -c build/rollup.dev.js --watch",
        "build": "cross-env NODE_ENV=production node build/rollup.prod.js",
        "docs:dev": "vuepress dev docs --temp docs/.temp",
        "docs:build": "vuepress build docs",
        "commit": "git-cz",
        "versions": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "lint-staged": "pretty-quick --staged",
        "test": "jest --coverage"
    },
    "author": "Hyhello",
    "license": "MIT",
    "bugs": {
        "url": "https://code.cestc.cn/cestc_wh/data_center/web-frontEnd/ball/-/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://code.cestc.cn/cestc_wh/data_center/web-frontEnd/ball.git"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "**/*.{js|jsx|ts|tsx}": [
            "eslint --ext .ts,.jsx,.ts,tsx",
            "prettier --write",
            "git add ."
        ]
    },
    "engines": {
        "node": ">=4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.0.0",
        "@babel/eslint-parser": "^7.15.0",
        "@babel/generator": "^7.15.0",
        "@babel/plugin-transform-modules-commonjs": "^7.15.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/runtime": "^7.8.4",
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@rollup/plugin-alias": "^3.1.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-eslint": "^8.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-typescript": "^8.2.5",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
        "@vue/babel-preset-jsx": "^1.2.4",
        "@vuepress/plugin-back-to-top": "^1.8.2",
        "@vuepress/plugin-google-analytics": "^1.8.2",
        "@vuepress/plugin-medium-zoom": "^1.8.2",
        "@vuepress/plugin-pwa": "^1.8.2",
        "@vuepress/theme-vue": "^1.8.2",
        "autoprefixer": "^10.3.2",
        "babel-jest": "^27.0.6",
        "babel-plugin-import": "^1.13.3",
        "chalk": "^4.1.2",
        "commitizen": "^4.2.4",
        "conventional-changelog-cli": "^2.1.1",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.8",
        "del": "^6.0.0",
        "element-ui": "^2.15.6",
        "eslint": "7.5.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-vue": "^7.16.0",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.2",
        "jest": "^27.0.6",
        "jest-environment-jsdom": "^27.0.6",
        "lint-staged": "^11.1.2",
        "node-sass": "^6.0.1",
        "portfinder": "^1.0.28",
        "postcss": "^8.2.15",
        "prettier": "^2.3.2",
        "pretty-quick": "^3.1.1",
        "rollup": "^2.56.3",
        "rollup-plugin-filesize": "^9.1.1",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-postcss": "^4.0.1",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "4.7.2",
        "tslib": "*",
        "typescript": "^3.7.0",
        "vue-eslint-parser": "^7.10.0",
        "vue-template-compiler": "^2.6.14",
        "vuepress": "^1.8.2",
        "vuepress-plugin-flowchart": "^1.4.3"
    },
    "peerDependencies": {
        "vue": "^2.6.11"
    },
    "dependencies": {
        "vue": "^2.6.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
